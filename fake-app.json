{
  "expo": {
    "name": "Walker Treker",
    "slug": "walkertreker",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "entryPoint": "./src/App.js",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash2.png",
      "resizeMode": "contain",
      "backgroundColor": "#8b0000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.eyecuelab.walkertreker"
    },
    "android": {
      "package": "com.eyecuelab.walkertreker",
      "googleServicesFile": "./google-services.json",
      "config": {
        "googleSignIn": {
          "certificateHash": "XXXXX",
          "apiKey": "XXXXX"
        },
      },
    },
    "scheme": "walkertreker",
  }
}
